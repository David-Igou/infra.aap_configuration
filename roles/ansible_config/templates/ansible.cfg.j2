{% for section in ansible_config_list %}
[{{ section.header }}]
{% for key_set in section.keypairs %}
{{ key_set.key }}={{ key_set.value }}
{% endfor %}
{% endfor %}

{% for server in automation_hub_list %}
[galaxy_server.{{ server.name }}]
{% if server.url is defined %}
url={{ server.url }}
{% endif %}
{% if server.auth_url is defined %}
auth_url={{ server.auth_url }}
{% endif %}
{% if server.token is defined %}
token={{ server.token }}
{% endif %}
{% if server.username is defined %}
username={{ server.username }}
{% endif %}
{% if server.password is defined %}
password={{ server.password }}
{% endif %}
{% endfor %}